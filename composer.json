{
    "name": "draw/security",
    "description": "Security related class compatible with symfony/security* component",
    "license": "MIT",
    "type": "library",
    "keywords": ["draw", "symfony", "security"],
    "authors": [
        {
            "name": "Martin Poirier Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "draw/core": "^0.9.5",
        "draw/framework-extra-bundle": "^0.9.5",
        "draw/messenger": "^0.9.5",
        "draw/tester": "^0.9.5",
        "firebase/php-jwt": "^6.1",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "symfony/messenger": "^5.4.3",
        "symfony/security-http": "^5.4.3",
        "symfony/security-core": "^5.4.3",
        "symfony/translation-contracts": "^2.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Component\\Security\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        }
    }
}
